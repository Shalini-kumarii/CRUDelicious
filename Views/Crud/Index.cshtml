@model Crud;
@using CRUDelicious.Models;
@using System.Collections.Generic;

<!DOCTYPE html>
<html>

<head>
    <meta charset='utf-8'>
    <title>Hello!</title>
    <link rel="stylesheet" href="~/css/style.css" />
</head>

<body>
<h1> Check out some recent dishes !</h1>

    @{
        foreach(var x in @ViewBag.DishList){
            <p><a href="@x.DishId">@x.Name</a> by @x.Chef</p>
        }
    }
    <a href="/new">Add a dish</a>
</body>
</html>
